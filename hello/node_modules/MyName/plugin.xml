<?xml version='1.0' encoding='utf-8'?>
<plugin id="MyId" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
	<name>MyName</name>
	<js-module name="MyName" src="www/MyName.js">
		<clobbers target="MyName" />
	</js-module>
	<platform name="android">
		<config-file parent="/*" target="res/xml/config.xml">
			<feature name="MyName">
				<param name="android-package" value="MyId.MyName" />
			</feature>
		</config-file>
		<config-file parent="/*" target="AndroidManifest.xml" />
		<source-file src="src/android/MyName.java" target-dir="src/MyId" />
	</platform>
	<platform name="ios">
		<config-file parent="/*" target="config.xml">
			<feature name="MyName">
				<param name="ios-package" value="MyName" />
			</feature>
		</config-file>
		<source-file src="src/ios/MyName.m" />
	</platform>
</plugin>